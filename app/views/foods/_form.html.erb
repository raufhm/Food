<%= form_for @food , html: {class: 'form-horizontal'} do |f| %>
<%= f.label :nama, 'Nama Makanan :', class: 'control-label'%>
<%= f.text_field :nama, class: 'form-control'%>

<%= f.label :harga, 'Harga :', class: 'control-label'%>
<%= f.text_field :harga, class: 'form-control'%>

<%= f.submit 'Simpan', class: 'btn btn-primary'%>
<% end %>